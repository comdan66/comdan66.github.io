=== no npm installed
git add -A && git commit -m 'Fix Html Code.' && git push origin dev && git checkout master && cd cmd && sudo npm install .  && gulp minify && gulp gh-pages && cd ../ && git add -A && git commit -m 'Minify js、html, fix gh-pages path bug.' && git push origin master --force && git checkout dev

=== has npm installed
git add -A && git commit -m 'Fix Html Code.' && git push origin dev && git checkout master && cd cmd && gulp minify && gulp gh-pages && cd ../ && git add -A && git commit -m 'Minify js、html, fix gh-pages path bug.' && git push origin master --force && git checkout dev